{
	"name": "quetzal",
	"displayName": "quetzal-koatl",
	"description": "Koatl Language Support",
	"repository": {
		"type": "git",
		"url": "https://github.com/skkestrel/koatl"
	},
	"version": "0.1.1",
	"publisher": "koatl",
	"engines": {
		"vscode": "^1.100.0"
	},
	"categories": [
		"Programming Languages",
		"Formatters"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "koatl",
				"aliases": [
					"koatl",
					"tl"
				],
				"extensions": [
					".tl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "koatl",
				"scopeName": "source.koatl",
				"path": "./syntaxes/tl.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"build": "npm run compile && CARGO_TARGET_DIR=./target cargo build --release --target wasm32-unknown-unknown",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"lint": "eslint",
		"esbuild": "node ./bin/esbuild.js",
		"generate:model": "wit2ts --outDir ./src ./wit",
		"serve": "serve --cors -l 5000 --ssl-cert $HOME/certs/localhost.pem --ssl-key $HOME/certs/localhost-key.pem"
	},
	"dependencies": {
		"@vscode/wasm-component-model": "^1.0.2"
	},
	"devDependencies": {
		"@types/vscode": "^1.100.0",
		"generator-code": "^1.11.10",
		"serve": "^14.2.1",
		"typescript": "^5.9.2"
	}
}
