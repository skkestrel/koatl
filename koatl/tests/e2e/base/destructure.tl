import util.assert_eq

a = 1
assert_eq(a, 1)

[a, b] = [1, 2]
assert_eq([a, b], [1, 2])

[a, *b, c] = [1, 2, 3, 4]
assert_eq([a, b, c], [1, [2, 3], 4])

[a, [b, [c, *d]]] = [1, [2, [3, 4, 5]]]
assert_eq([a, b, c, d], [1, 2, 3, [4, 5]])

{1: a, 2: b} = {1: "1", 2: "2"}
assert_eq([a, b], ["1", "2"])

{1: a, **b} = {1: "1", 2: "2", 3: "3"}
assert_eq([a, b], ["1", {2: "2", 3: "3"}])

{1: [a, *b], **c} = {1: [1, 2, 3], 2: "2", 3: "3"}
assert_eq([a, b, c], [1, [2, 3], {2: "2", 3: "3"}])

{1: [{"a": a}, *b], **c} = {1: [{"a": "a"}, 2, 3], 2: "2", 3: "3"}
assert_eq([a, b, c], ["a", [2, 3], {2: "2", 3: "3"}])

[a, b] = [1, 2]
assert_eq([a, b], [1, 2])

[a, *b, c] = [1, 2, 3, 4]
assert_eq([a, b, c], [1, [2, 3], 4])

[a, [b, [c, *d]]] = [1, [2, [3, 4, 5]]]
assert_eq([a, b, c, d], [1, 2, 3, [4, 5]])

{1: a, 2: b} = {1: "1", 2: "2"}
assert_eq([a, b], ["1", "2"])

{1: a, **b} = {1: "1", 2: "2", 3: "3"}
assert_eq([a, b], ["1", {2: "2", 3: "3"}])

{1: [a, *b], **c} = {1: [1, 2, 3], 2: "2", 3: "3"}
assert_eq([a, b, c], [1, [2, 3], {2: "2", 3: "3"}])

{1: [{"a": a}, *b], **c} = {1: [{"a": "a"}, 2, 3], 2: "2", 3: "3"}
assert_eq([a, b, c], ["a", [2, 3], {2: "2", 3: "3"}])

{a, **b} = {a: 42, b: 43}
assert_eq([a, b], [42, {b: 43}])

a, [*b], c, *d = 1, [2, 3], 4, 5, 6
assert_eq([a, b, c, d], [1, [2, 3], 4, [5, 6]])