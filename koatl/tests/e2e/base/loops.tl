import util.assert_eq

x = []
for i in [1, 2, 3]:
    x = [*x, i]

assert_eq(x, [1, 2, 3])

i = True
while (x => x)(i):
    if i === False:
        raise

    i = False

counts = []
counter = 0
while (
    counter = counter + 1
    counter < 5
):
    counts.append(counter)

assert_eq(counter, 5)
assert_eq(counts, [1, 2, 3, 4])